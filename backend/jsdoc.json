{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["controllers", "models", "routes", "middleware"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  }
}

